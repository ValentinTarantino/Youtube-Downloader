{
"version": 2,
"builds": [
    {
    "src": "youtube-downloader-frontend/package.json",
    "use": "@vercel/static-build",
    "config": {
        "distDir": "dist"
    }
    },
    {
    "src": "/api/(.*)",
    "dest": "/api/index.js"
    }
],
    "routes": [
    {
    "src": "/api/(.*)",
    "dest": "api/index.js"
    },
    {
    "src": "/(.*)",
    "dest": "youtube-downloader-frontend/dist/index.html"
    }
]
}